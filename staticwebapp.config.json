{
  "routes": [
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/api/auth-status",
      "allowedRoles": ["authenticated", "anonymous"]
    },
    {
      "route": "/api/sso-auth-status",
      "allowedRoles": ["authenticated", "anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    }
  ]
} 